{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "interface-name": false,
        "quotemark": [true, "single"],
        "no-empty-interface": false,
        "no-console": false,
        "object-literal-sort-keys": false,
        "member-ordering": false,
        "eofline": false
    },
    "rulesDirectory": [
        "src/ts"
    ]
}